{
  "name": "classnames-in-js",
  "author": "Ragnar Rognstad (https://rognstad.me)",
  "repository": "rognstadragnar/classnames-in-js",
  "license": "MIT",
  "version": "0.1.3",
  "source": "src/index.js",
  "main": "dist/classnames-in-js.js",
  "module": "dist/classnames-in-js.esm.js",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "chai": "^4.1.2",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "microbundle": "^0.9.0",
    "mocha": "^5.1.1",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "preact": "^8.4.2",
    "prettier": "^1.10.2",
    "react": "^16.8.1",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": "*",
    "preact": "*"
  },
  "scripts": {
    "release": "np",
    "bundle": "npm-run-all bundle:*",
    "bundle:react": "microbundle",
    "bundle:preact-cjs": "microbundle src/preact.js -o preact.js -f cjs --compress",
    "bundle:preact-es": "microbundle src/preact.js -o preact.js -f es --compress",
    "dev": "microbundle watch --compress false",
    "verify:lint": "eslint ./src",
    "verify:test": "mocha -r @babel/register __tests__/**/*.spec.js",
    "test": "npm run verify:lint && npm run verify:test",
    "prepare": "npm run build",
    "build": "npm run clean && npm run bundle",
    "clean": "npm-run-all clean:*",
    "clean:react": "rimraf dist",
    "clean:preact": "rimraf dist preact.js preact.js.map preact.esm.js preact.esm.js.map preact.umd.js preact.umd.js.map"
  },
  "files": [
    "src",
    "dist",
    "preact.js",
    "preact.js.map",
    "preact.esm.js",
    "preact.esm.js.map",
    "preact.umd.js",
    "preact.umd.js.map"
  ]
}
